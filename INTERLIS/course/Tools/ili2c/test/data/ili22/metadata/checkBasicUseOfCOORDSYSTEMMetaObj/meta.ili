INTERLIS 2.2;

REFSYSTEM MODEL Meta =

    STRUCTURE LengthAXIS EXTENDS INTERLIS.AXIS =
    PARAMETER
      Unit (EXTENDED): NUMERIC [INTERLIS.LENGTH];
    END LengthAXIS;

	TOPIC Refsystem =
		CLASS GeoCartesian2D  EXTENDS INTERLIS.COORDSYSTEM =
			Axis (EXTENDED): LIST {2} OF LengthAXIS;
		END GeoCartesian2D; 
	END Refsystem;
END Meta.
