INTERLIS 2.2;

MODEL model =
	TOPIC topic =

		STRUCTURE SubItem =
			attr1 : TEXT*10;
			attr2 : TEXT*10;
		END SubItem;

		STRUCTURE Item =
			subitems : BAG {0..*} OF SubItem;
		END Item;

		CLASS aclass =
			items : BAG {0..*} OF Item;
			UNIQUE (LOCAL) items->subitems:attr1,attr2;
		END aclass;
		

	END topic;

END model.
