INTERLIS 2.3;

MODEL ModelA (de) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" =

	TOPIC TopicA1 =
	
		CLASS ClassA1 =
			attrA1 : BOOLEAN;
		END ClassA1;
		
		ASSOCIATION AssocA3 =
			roleA1 -- ClassA1;
			roleA2 -- ClassA1;
			MANDATORY CONSTRAINT roleA1->attrA1;
		END AssocA3;
		
	END TopicA1;
	
END ModelA.


MODEL ModelB (fr) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" 
TRANSLATION OF ModelA [ "1"]
=
	TOPIC TopicB1 =
	
		CLASS ClassB1 =
			attrB1 : BOOLEAN;
		END ClassB1;
		
		ASSOCIATION AssocB3 =
			roleB1 -- ClassB1;
			roleB2 -- ClassB1;
			MANDATORY CONSTRAINT roleB2->attrB1;
		END AssocB3;
		
	END TopicB1;

END ModelB.
