INTERLIS 2.2;

MODEL model =

  TOPIC topic =

    CLASS a =
      farbe : (rot,
           gelb,
           gruen);
    END a;
    CLASS b EXTENDS a=
      farbe (EXTENDED): (rot (dunkel, karmin, orange),
                             gruen (hell, dunkel: FINAL),
                             blau);
    END b;
  END topic;
END model.
