INTERLIS 2.3;

MODEL ModelA (de) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" =

	TOPIC TopicA1 =
	
		CLASS ClassA1 =
		END ClassA1;
		
		ASSOCIATION AssocA3 =
			roleA1 -- {1} ClassA1;
			roleA2 -- {1} ClassA1;
			attrA1 : BOOLEAN;
		END AssocA3;
		
		CONSTRAINTS OF ClassA1 =
			MANDATORY CONSTRAINT \roleA1->attrA1;
		END;
		
	END TopicA1;
	
END ModelA.


MODEL ModelB (fr) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" 
TRANSLATION OF ModelA [ "1"]
=
	TOPIC TopicB1 =
	
		CLASS ClassB1 =
		END ClassB1;
		
		ASSOCIATION AssocB3 =
			roleB1 -- {1} ClassB1;
			roleB2 -- {1} ClassB1;
			attrB1 : BOOLEAN;
		END AssocB3;
		
		CONSTRAINTS OF ClassB1 =
			MANDATORY CONSTRAINT \roleB2->attrB1;
		END;
		
	END TopicB1;

END ModelB.
