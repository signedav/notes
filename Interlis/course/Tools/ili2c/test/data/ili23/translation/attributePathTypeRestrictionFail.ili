INTERLIS 2.3;

MODEL ModelA (de) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" =
	
	TOPIC TopicA =
		
		CLASS ClassA =
			attrA1 : ATTRIBUTE;
			attrA2 : ATTRIBUTE RESTRICTION (TEXT*10);
		END ClassA;

	END TopicA;

END ModelA.

MODEL ModelB (fr) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" 
TRANSLATION OF ModelA [ "1"]
=

	TOPIC TopicB =

		CLASS ClassB =
			attrB1 : ATTRIBUTE;
			attrB2 : ATTRIBUTE RESTRICTION (1..10);
		END ClassB;

	END TopicB;
	
END ModelB.
