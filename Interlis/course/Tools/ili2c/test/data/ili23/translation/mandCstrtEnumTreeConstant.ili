INTERLIS 2.3;

MODEL ModelA (de) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" =

	DOMAIN	
		DomainA1 = (a1,a2(a21,a22));
		DomainA2 = ALL OF DomainA1;
	TOPIC TopicA1 =
	
		CLASS ClassA1 =
			attrA1 : DomainA2;
			MANDATORY CONSTRAINT attrA1==#a2;
		END ClassA1;
		
	END TopicA1;
	
END ModelA.


MODEL ModelB (fr) AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" 
TRANSLATION OF ModelA [ "1"]
=
	DOMAIN
		DomainB1 = (b1,b2(b21,b22));
		DomainB2 = ALL OF DomainB1;
	TOPIC TopicB1 =
	
		CLASS ClassB1 =
			attrB1 : DomainB2;
			MANDATORY CONSTRAINT attrB1==#b2;
		END ClassB1;
		
	END TopicB1;

END ModelB.
