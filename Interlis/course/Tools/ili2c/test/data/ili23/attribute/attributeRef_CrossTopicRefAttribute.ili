INTERLIS 2.3;

MODEL Model AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" =

	TOPIC TopicA =

		CLASS a =
		END a;

	END TopicA;

	TOPIC TopicC EXTENDS TopicA = 

