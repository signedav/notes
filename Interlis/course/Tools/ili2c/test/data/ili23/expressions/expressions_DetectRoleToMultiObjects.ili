INTERLIS 2.3;


MODEL Model AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" =


  TOPIC Topic = 

    CLASS A =
      attr : TEXT*10;
    END A;

    CLASS B =
    END B;

    ASSOCIATION  =
      a -- {1..*} A;
      b -- B;
    END;

    CONSTRAINTS OF B =
      MANDATORY CONSTRAINT a->attr=="Hello World"; !! illegal, may point to multiple objects
    END;

  END Topic;



END Model.
